@model Vendor
@{
    ViewData["Title"] = "Vendor Page";
}

<div class="text-center">
    <h1 class="display-4">Vendors</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col" class="text-nowrap">Name</th>
                <th scope="col" class="text-nowrap">Vendor Phone</th>
                <th scope="col" class="text-nowrap">Vendor Contact<br/>Last Name</th>
                <th scope="col" class="text-nowrap">Vendor Contact<br/>First Name</th>
                <th scope="col" class="text-nowrap">Vendor Contact Email</th>
                <th scope="col"><br/></th>
                <th scope="col"><br/></th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {

                <tr scope="row">
                    <td>@Model.Name</td>
                    <td class="text-nowrap">@Model.VendorPhone</td>
                    <td>@Model.VendorContactLastName</td>
                    <td>@Model.VendorContactFirstName</td>
                    <td>@Model.VendorContactEmail</td>
                </tr>
                
            }
        </tbody>
    </table>
</div>
